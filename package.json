{
   "name": "web-ignition",
   "description": "Start a fire",
   "version": "1.1.4",
   "license": "MIT",
   "main": "dist/library.js",
   "browser": "dist/library.min.js",
   "style": "dist/reset.min.css",
   "repository": "github:center-key/web-ignition",
   "homepage": "https://github.com/center-key/web-ignition",
   "bugs": "https://github.com/center-key/web-ignition/issues",
   "docs": "https://github.com/center-key/web-ignition#readme",
   "keywords": [
      "css",
      "css-reset",
      "html",
      "javascript",
      "layouts",
      "web"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "browser": true,
      "jquery": true,
      "node": true,
      "globals": {
         "dna": false
      }
   },
   "scripts": {
      "pretest": "jshint *.js js css/layouts && gulp validate-specs",
      "test": "gulp clean && gulp build-css && gulp build-js && gulp build-layouts"
   },
   "dependencies": {
      "@fortawesome/fontawesome-free": "~5.7",
      "dna.js": "~1.5",
      "fetch-json": "~2.2",
      "jquery": "~3.3"
   },
   "devDependencies": {
      "@babel/core": "~7.2",
      "@babel/preset-env": "~7.3",
      "babel-minify": "~0.5",
      "cssnano": "~4.1",
      "del": "~3.0",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.2",
      "gulp-less": "~4.0",
      "gulp-postcss": "~8.0",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~1.4",
      "highlight.js": "~9.14",
      "jshint": "~2.10",
      "merge-stream": "~1.0",
      "postcss-font-magician": "~2.2",
      "postcss-preset-env": "~6.5"
   }
}
